<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field
			id="id"
			name="id"
			type="text"
			default="0"
			readonly="true"
			class="readonly"
			label="JGLOBAL_FIELD_ID_LABEL"
			description ="JGLOBAL_FIELD_ID_DESC" />
		<field
			name="log_type"
			type="text"
			class="readonly"
			label="COM_LOGS_FIELD_TYPE_LABEL"
			description="COM_LOGS_FIELD_TYPE_DESC"
			required="true"
			size="40"
			rows="10"
			cols="80"
			style="width:55%"/>
		<field
			name="http_status"
			type="text"
			class="readonly"
			label="COM_LOGS_FIELD_HTTP_STATUS_LABEL"
			description="COM_LOGS_FIELD_HTTP_STATUS_DESC"
			required="true"
			size="40"
			rows="10"
			cols="80"
			style="width:55%"/>
			<field
				name="http_status_text"
				type="text"
				class="readonly"
				label="COM_LOGS_FIELD_HTTP_STATUS_TEXT_LABEL"
				description="COM_LOGS_FIELD_HTTP_STATUS_TEXT_DESC"
				required="true"
				size="40"
				rows="10"
				cols="80"
				style="width:55%"/>
		<field
			name="http_response_text"
			type="textarea"
			class="readonly"
			label="COM_LOGS_FIELD_HTTP_RESPONSE_TEXT_LABEL"
			description="COM_LOGS_FIELD_HTTP_RESPONSE_TEXT_DESC"
			required="true"
			rows="10"
			cols="200"
			/>	
		<field
			name="entity_type"
			type="textarea"
			class="readonly"
			label="COM_LOGS_FIELD_ENTITY_TYPE_LABEL"
			description="COM_LOGS_FIELD_ENTITY_TYPE_DESC"
			rows="10"
			cols="200"
			/>	
		<field
			name="entity_id"
			type="textarea"
			class="readonly"
			label="COM_LOGS_FIELD_ENTITY_ID_LABEL"
			description="COM_LOGS_FIELD_ENTITY_ID_DESC"
			rows="10"
			cols="200"
			/>	
		<field
			name="event"
			type="textarea"
			class="readonly"
			label="COM_LOGS_FIELD_EVENT_LABEL"
			description="COM_LOGS_FIELD_EVENT_DESC"
			rows="10"
			cols="200"
			/>
			<field
				name="event_status"
				type="textarea"
				class="readonly"
				label="COM_LOGS_FIELD_ENVENT_STATUS_LABEL"
				description="COM_LOGS_FIELD_ENVENT_STATUS_DESC"
				rows="10"
				cols="200"
				/>			
		<field
			name="content"
			type="textarea"
			class="readonly"
			label="COM_LOGS_FIELD_CONTENT_LABEL"
			description="COM_LOGS_FIELD_CONTENT_DESC"
			required="true"
			rows="10"
			cols="200"
			/>

		<field
			name="create_time"
			type="hidden"
			id="create_time"
			class="readonly"
			label="COM_LOGS_FIELD_CREATED_DATE_LABEL"
			size="20"
			readonly="true" />
		<field
			name="published"
			type="hidden"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			class="inputbox"
			size="1"
			required="true"
			default="1" />
	</fieldset>
</form>
